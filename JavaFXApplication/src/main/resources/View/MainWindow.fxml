<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="AnchorPane" prefHeight="500.0" prefWidth="392.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="javafxapplication.Controller.MainController">
  <children>
    <MenuBar>
      <menus>
        <Menu text="Сатуучулар">
          <MenuItem mnemonicParsing="false" onAction="#addSeller" text="Сатуучу кошуу" />
          <MenuItem mnemonicParsing="false" onAction="#listSeller" text="Сатуучулардын тизмеси" />
        </Menu>
        <Menu text="Дукондор">
            <MenuItem mnemonicParsing="false" text="Дукон кошуу" onAction="#addShop"/>
          <MenuItem mnemonicParsing="false" onAction="#listShop" text="Дукондордун тизмеси" />
            <MenuItem mnemonicParsing="false" onAction="#listShopTreeView" text="Дукондордун жана сатуучулардын тизмеси"></MenuItem>
        </Menu>
          <Menu text="Буйумдар">
              <items>
                  <MenuItem mnemonicParsing="false" text="Сатылган буюмдардын тизмеси" onAction="#listSoldProducts"/>
              </items>
          </Menu>
      </menus>
    </MenuBar>
    <VBox fx:id="vbox">
      <VBox.margin>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
      </VBox.margin>
    </VBox>
  </children>
</VBox>
